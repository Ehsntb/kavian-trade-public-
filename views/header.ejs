            </div>
            <!-- /Menu Toggle -->
          </div>
          <div class="col-auto hdr-logo">
            <a href="/" class="logo"
              ><img
                srcset="/images/logo-icon.png 1x"
                alt="Logo"
                height="60px"
                width="60px"
            /></a>
          </div>
          <!--navigation-->

          <div class="hdr-nav hide-mobile nav-holder justify-content-center">
            <!--mmenu-->
            <ul class="mmenu mmenu-js">
              <li class="mmenu-item--simple">
                <a href="/">صفحه اصلی</a>
              </li>
              <li class="mmenu-item--simple">
                <a href="/product">همه محصولات</a>
              </li>
              <li class="mmenu-item--simple">
                <a href="">دسته بندی محصولات</a>
                <div class="mmenu-submenu">
                  <% if (categories != "not found") { %>
                  <ul class="submenu-list">
                    <% for (category of categories) { %> <% if
                    (category.parent_id == null) { %>
                    <a href="/productc/<%= category.short_link %>"
                      ><b><%= category.title %></b></a
                    >
                    <ul>
                      <% for (subcategory of categories) { %> <% if
                      (subcategory.parent_id == category.id) { %>
                      <li>
                        <a href="/productc/<%= subcategory.short_link %>"
                          ><%= subcategory.title %></a
                        >
                      </li>
                      <% } } %>
                    </ul>
                    <% } else if (category.parent_id == null) { %>
                    <li>
                      <a href="/productc/<%= category.short_link %>"
                        ><b><%= category.title %></b></a
                      >
                    </li>
                    <% } } %>
                  </ul>
                  <% } else { %>
                  <a>not found</a>
                  <% } %>
                </div>
              </li>
              <li class="mmenu-item--simple">
                <a href="/contact_us">ارتباط با ما</a>
              </li>
              <li class="mmenu-item--simple">
                <a href="/about_us">درباره ما</a>
              </li>
              <li class="mmenu-item--simple">
                <a href="/login">ورود به پنل مدیر</a>
              </li>
            </ul>

            <!--/mmenu-->
          </div>
          <!--//navigation-->
        </div>
      </div>
    </div>
